{
	"name": "orchestra/kernel",
	"description": "Kernel for Orchestra Platform",
	"license": "MIT",
	"authors": [
		{
			"name": "Mior Muhammad Zaki",
			"email": "crynobone@gmail.com",
			"homepage": "https://github.com/crynobone"
		}
	],
	"autoload": {
		"psr-4": {
			"Orchestra\\Contracts\\": "src/Contracts/",
			"Orchestra\\Http\\": "src/Http/",
			"Orchestra\\Routing\\": "src/Routing/"
		}
	},
	"require": {
		"php": ">=5.4.0",
		"illuminate/contracts": "5.0.*",
		"orchestra/support-core": "3.0.*"
	},
	"require-dev": {
		"mockery/mockery": "0.9.*",
		"laravel/framework": "5.0.*"
	},
	"replace": {
		"orchestra/contracts": "self.version",
		"orchestra/http": "self.version"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.0-dev"
		}
	},
	"minimum-stability": "dev"
}
